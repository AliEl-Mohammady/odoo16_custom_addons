<?xml version="1.0" encoding="UTF-8"?>

<templates xml:space="preserve">
    <t t-inherit="web.DateField" t-inherit-mode="extension" owl="1">
        <xpath expr="//DatePicker" position="after">
            <span t-if="props.value" t-out="calender.islamic"/>
        </xpath>
    </t>
    <t t-inherit="web.DateTimeField" t-inherit-mode="extension" owl="1">
        <xpath expr="//DateTimePicker" position="after">
            <span t-if="props.value" t-out="calender.islamic"/>
        </xpath>
    </t>
</templates>